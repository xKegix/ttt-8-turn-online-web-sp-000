#!/usr/bin/env ruby
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]

puts "Welcome to Tic Tac Toe!"
puts "  |  |  "
puts "--------"
puts "  |  |  "
puts "--------"
puts "  |  |  "
print "Please enter 1-9:"

user_input = gets.strip

input_to_index(user_input)

valid_move?(board, index)

position_taken?(board, index)

move(board, index, player)


turn(board)
